@model List<Glosowanie.Models.Question>

@for (int i = 0; i < Model.Count; i++)
{
    <div class="stepImg">@Html.TextBoxFor(model => model[i].QuestionText, new { placeholder = "Podaj pytanie", @class = "input", id = String.Format("QestionText{0}", i)})</div>
    
    Html.RenderPartial("~/Views/Partials/AddAnswers.cshtml", Model[i].Answers);
}